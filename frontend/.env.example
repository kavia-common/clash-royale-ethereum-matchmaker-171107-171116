# Frontend environment variables
# Copy this file to ".env" and fill in values.

# Base URL of the backend API (no trailing slash), e.g. http://localhost:8000 or https://api.example.com
REACT_APP_API_URL=http://localhost:8000

# Ethereum escrow smart contract address for deposits
REACT_APP_ESCROW_ADDRESS=0xYourEscrowContractAddressHere
